{
  "name": "foodstop",
  "version": "1.0.0",
  "description": "a food ordering and delivery api",
  "main": "build/app.js",
  "imports": {
    "#src/*": "./build/*.js",
    "#config/*": "./build/config/*.js",
    "#controllers/*": "./build/controllers/*.js",
    "#models/*": "./build/models/*.js",
    "#routes": "./build/routes/index.js",
    "#routes/*": "./build/routes/*.js"
  },
  "scripts": {
    "start": "node build/app.js",
    "dev": "nodemon --exec npx ts-node src/app.ts",
    "test": "mocha --timeout 10000 -r ts-node/register ./src/__tests__/**/*.test.ts",
    "postinstall": "tsc",
    "build": "rm -rf ./build && tsc"
  },
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.31",
    "@types/supertest": "^2.0.12",
    "chai": "^4.3.6",
    "dotenv": "^16.0.0",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.16",
    "supertest": "^6.2.3",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "express": "^4.18.1",
    "ioredis": "^5.0.4",
    "mongoose": "^6.3.2"
  }
}
